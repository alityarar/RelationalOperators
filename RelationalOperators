package day06_concatenation_Operators;

public class C02_RelationalOperators {
    public static void main(String[] args) {

    
        boolean num1=5+2==7;  //True
        System.out.println(!num1); //False yaptik
        boolean num2=5*2==15; //False


        boolean ex1=5+2 !=7;  //False
        System.out.println(5*2 !=15);   //True
    }
}
